<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="900.0" styleClass="background-pane" stylesheets="@../../../styles/fishingmanagement.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.students.sri_lanka_marine_life_explorer.FishingManagementController">
