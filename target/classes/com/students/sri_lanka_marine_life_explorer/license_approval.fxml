<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="900.0" stylesheets="@../../../styles/license_approval.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.students.sri_lanka_marine_life_explorer.LicenseApprovalController">
   <children>
      <BorderPane prefHeight="700.0" prefWidth="900.0">
         <top>
            <Label prefHeight="64.0" prefWidth="306.0" text="Pending License Approvals" BorderPane.alignment="CENTER" />
         </top>
         <center>
            <TableView fx:id="licenseTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="boatColumn" prefWidth="135.0" text="Boat Info" />
                <TableColumn fx:id="typeColumn" prefWidth="142.0" text="Fishing Type" />
                <TableColumn fx:id="issueDateColumn" minWidth="0.0" prefWidth="154.0" text="Issue Date" />
                <TableColumn fx:id="expiryDateColumn" prefWidth="151.0" text="Expiry Date" />
                <TableColumn fx:id="regionColumn" prefWidth="151.0" text="Region" />
                <TableColumn fx:id="statusColumn" minWidth="0.0" prefWidth="142.0" text="Status" />
            </columns>
            </TableView>
         </center>
         <bottom>
            <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="approveButton" mnemonicParsing="false" text="Approve">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin></Button>
                  <Button fx:id="rejectButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="108.0" text="Reject">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin></Button>
                  <Button mnemonicParsing="false" onAction="#goToPasswordChange" text="Change Password">
                     <HBox.margin>
                        <Insets left="90.0" />
                     </HBox.margin></Button>
                  <Button mnemonicParsing="false" onAction="#logOut" text="Log Out">
                     <HBox.margin>
                        <Insets left="40.0" />
                     </HBox.margin></Button>
                  <Button mnemonicParsing="false" onAction="#goToMainMenu" text="Main Menu">
                     <HBox.margin>
                        <Insets left="40.0" />
                     </HBox.margin></Button>
               </children>
               <padding>
                  <Insets top="30.0" />
               </padding>
            </HBox>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
